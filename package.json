{
  "name": "nunjucks-html-loader",
  "version": "1.1.0",
  "description": "A webpack loader that takes nunjucks templates and returns raw html. It also works with webpack's watch command",
  "npmName": "nunjucks-html-loader",
  "keywords": [
    "webpack",
    "nunjucks",
    "html",
    "raw",
    "watch",
    "hmr",
    "live"
  ],
  "author": "Ryan Hornberger <ryhornberger@gmail.com> (https://github.com/ryanhornberger)",
  "license": "MIT",
  "homepage": "https://github.com/ryanhornberger/nunjucks-html-loader",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanhornberger/nunjucks-html-loader.git"
  },
  "bugs": {
    "url": "https://github.com/ryanhornberger/nunjucks-html-loader/issues"
  },
  "main": "index.js",
  "dependencies": {
    "loader-utils": "^0.2.16",
    "nunjucks": "^3.0.0"
  },
  "devDependencies": {
    "commitlint-config-imt": "0.0.1",
    "eslint-config-zman": "0.0.9"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "node_modules/commitlint-config-imt/cz-config.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
